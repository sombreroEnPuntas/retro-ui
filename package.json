{
  "name": "retro-ui",
  "version": "0.0.3",
  "license": "MIT",
  "description": "NES-style (8bit-like) react & styled-components UI",
  "homepage": "https://sombreroenpuntas.github.io/retro-ui/",
  "repository": "sombreroEnPuntas/retro-ui",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "jsnext:main": "build/index.es.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "rollup -c",
    "docs": "styleguidist build && cp favicon.ico docs/",
    "eject": "react-scripts eject",
    "lint": "eslint . --ignore-path .gitignore",
    "start": "styleguidist server",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app",
    "ignorePatterns": [
      "docs/",
      "**/*.md"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^16.12.0",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^4.4.1",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "react-styleguidist": "^10.4.0",
    "rollup": "^1.27.14",
    "rollup-plugin-typescript2": "^0.25.3",
    "typescript": "~3.7.2"
  },
  "peerDependencies": {}
}
